#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules/openai/bin/node_modules:/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules/openai/node_modules:/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules:/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules/openai/bin/node_modules:/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules/openai/node_modules:/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules:/Users/wugaoyi/Learning/Project/ImageRecognitionTool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules/openai/bin/cli" "$@"
else
  exec node  "$basedir/../.pnpm/openai@5.0.1_ws@8.18.2_bufferutil@4.0.9__zod@3.25.36/node_modules/openai/bin/cli" "$@"
fi
